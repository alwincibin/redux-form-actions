{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "func-names": 0,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": ["error", "FunctionExpression", "WithStatement"],
    "react/prop-types": 0    
  }
}
